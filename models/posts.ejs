const mongoose = require('mongoose')

const postsSchema = new mongoose.Schema({
    const PostSchema = new mongoose.Schema({
        game:String,
        username:String,
        postDate:String,
        postTitle:String,
        image:String,
        postDesc:String,
        vote:Number,
        commentNo:Number,
        postid: {
          type: String,
          unique: true,
          required: true
          }
    
    }); 
})